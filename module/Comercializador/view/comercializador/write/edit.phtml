<h1>WriteController::editAction()</h1>
<?php
$form = $this->form;
$form->setAttribute('action', $this->url('comercializador/edit', array(), true));
$form->prepare();

$form->get('submit')->setValue('Update Comercializador');

echo $this->form()->openTag($form);

echo $this->formCollection($form);

echo $this->form()->closeTag();

